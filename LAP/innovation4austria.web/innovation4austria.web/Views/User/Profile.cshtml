@model innovation4austria.web.Models.ProfileModel

@{
    ViewBag.Title = "Profile";
}
<div class="container">
    <div class="row">
        <h2 class="col-lg-12">Profile</h2>
    </div>
    <div class="row">
        <div class="col-md-12">
            <h1>Profil verwalten</h1>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-6">
            @Html.Partial("_ProfileData", Model.ProfileData)
        </div>

        <div class="col-lg-6">
            @Html.Partial("_ProfilePassword", Model.ProfilePassword)
        </div>
    </div>
</div>